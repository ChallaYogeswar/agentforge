# AgentForge Environment Configuration
# Copy this file to .env and fill in your actual values
# NOTE: Never commit .env file to Git - it's ignored in .gitignore

# Google Gemini API Key (get it from https://aistudio.google.com)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Database path
DB_PATH=data/memory/sessions.db

# Optional: Vector store path
VECTOR_STORE_PATH=data/memory/vector_store
